{
    "name": "serge/pdo-query-builder",
    "description": "Package for easy using queries from database",
    "type": "library",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Serge\\PdoQueryBuilder\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Davut",
            "email": "sergeevdavut@mail.ru"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "ext-pdo": "*",
        "serge/dotenv": "@dev",
        "serge/config": "@dev"
    },
    "repositories": {
        "dotenv": {
            "type": "path",
            "url": "/OpenServer/domains/packages/dotenv",
            "options": {
                "symlink": true
            }
        },
        "config": {
            "type": "path",
            "url": "/OpenServer/domains/packages/config",
            "options": {
                "symlink": true
            }
        }
    }
}
